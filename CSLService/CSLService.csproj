<Project Sdk="Microsoft.NET.Sdk.Web">

	<PropertyGroup>
		<OutputType>Library</OutputType>
		<TargetFramework>net6.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<RootNamespace>FlyByWireless.CSLOnDemand</RootNamespace>
		<PackageId>FlyByWireless.CSLOnDemand</PackageId>
		<Authors>WONG Tin Chi Timothy</Authors>
		<Product>CSL On-Demand Service</Product>
		<Copyright>(C) 2021 WONG Tin Chi Timothy</Copyright>
		<Description>Serves CSLs on demand.</Description>
		<PackageTags>csl;x-plane;xplane;xcsl</PackageTags>
		<VersionPrefix>1.0.0</VersionPrefix>
		<VersionSuffix>alpha</VersionSuffix>
		<PackageLicenseFile>LICENSE</PackageLicenseFile>
	</PropertyGroup>

	<ItemGroup>
		<None Include="../LICENSE" Pack="true" PackagePath="" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="SharpCompress" Version="0.30.0" />
	</ItemGroup>

</Project>